{
  "$schema": "https://manifest.hypermode.com/hypermode.json",
  "models": {
    "minilm": {
      "sourceModel": "sentence-transformers/all-MiniLM-L6-v2",
      "provider": "hugging-face",
      "host": "hypermode"
    },
    "text-generator": {
      "sourceModel": "gpt-4o-mini",
      "host": "openai",
      "path": "v1/chat/completions"
    }
  },
  "hosts": {
    "openai": {
      "baseUrl": "https://api.openai.com/",
      "headers": {
        "Authorization": "Bearer {{API_KEY}}"
      }
    }
  },
  "collections": {
    "productNames": {
      "searchMethods": {
        "searchMethod1": {
          "embedder": "miniLMEmbed",
          "index": {
            "type": "sequential"
          }
        }
      }
    },
    "productDescriptions": {
      "searchMethods": {}
    },
    "productCategories": {
      "searchMethods": {}
    },
    "productPrices": {
      "searchMethods": {}
    },
    "productImages": {
      "searchMethods": {}
    },
    "productStars": {
      "searchMethods": {}
    },
    "isProductStocked": {
      "searchMethods": {}
    }
  }
}
